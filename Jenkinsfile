pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                sleep 10
            }
        }
         stage('build') {
            steps {
                echo 'Hello World'
                 sleep 10
            }
        }
         stage('test') {
            steps {
               sleep 10
                echo 'Hello World'
            }
        }
         stage('deploy') {
            steps {
                 sleep 10
                echo 'Hello World'
            }
        }
    }
}

